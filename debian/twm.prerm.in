set -e

THIS_PACKAGE=twm
THIS_SCRIPT=prerm

#INCLUDE_SHELL_LIB#

#DEBHELPER#

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
  update-alternatives --remove x-window-manager /usr/bin/twm
fi

exit 0

